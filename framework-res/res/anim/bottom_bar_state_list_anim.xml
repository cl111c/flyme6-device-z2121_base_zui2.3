<?xml version="1.0" encoding="utf-8"?>
<selector
  xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_pressed="true">
        <set>
            <objectAnimator android:interpolator="@interpolator/overshoot" android:duration="300" android:valueFrom="0.8" android:valueTo="1.0" android:valueType="floatType" android:propertyName="scaleY" />
            <objectAnimator android:interpolator="@interpolator/overshoot" android:duration="300" android:valueFrom="0.8" android:valueTo="1.0" android:valueType="floatType" android:propertyName="scaleX" />
        </set>
    </item>
    <item android:state_pressed="false">
        <set>
            <objectAnimator android:interpolator="@interpolator/overshoot" android:duration="300" android:valueFrom="0.8" android:valueTo="1.0" android:valueType="floatType" android:propertyName="scaleY" />
            <objectAnimator android:interpolator="@interpolator/overshoot" android:duration="300" android:valueFrom="0.8" android:valueTo="1.0" android:valueType="floatType" android:propertyName="scaleX" />
        </set>
    </item>
</selector>